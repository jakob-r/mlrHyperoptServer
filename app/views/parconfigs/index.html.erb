<p id="notice"><%= notice %></p>

<h1>ParConfigs</h1>

<table class="table table-striped table-hover parconfig">
  <thead>
    <tr>
      <th>id</th>
      <th>User email</th>
      <th>Learner type</th>
      <th>Learner name</th>
      <th>ParSet</th>
      <th>ParVals</th>
      <th>Default</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parconfigs.each do |parconfig| %>
      <tr>
        <td><%= parconfig.id %></td>
        <td><%= parconfig.anon_email %></td>
        <td><%= parconfig.learner_type %></td>
	<td><%= link_to parconfig.learner_name, parconfigs_path(:learner_name => parconfig.learner_name) %></td>
        <td><%= render 'parset', parset: parconfig.parset %></td>
        <td><%= render 'parvals', parvals: parconfig.parvals %></td>
        <td><%= parconfig.default %></td>
        <td><%= parconfig.note %></td>
        <td><%= link_to 'Show', parconfig %></td>
        <td><%= link_to 'Edit', edit_parconfig_path(parconfig) %></td>
        <td><%= link_to 'Destroy', parconfig, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parconfig', new_parconfig_path %> |
<%= link_to 'Back', parconfigs_path %>
